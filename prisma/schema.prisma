// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model DataProduk {
  //kolom               tipe data   option
  id          Int    @id @default(autoincrement())
  partnerName String
  productId   String
  operator    String
  productType String
  productName String
  basePrice   Int
}

model User {
  //kolom               tipe data   option
  id       Int    @id @default(autoincrement())
  username String @unique
  password String
  name     String
  email    String
  image    String
}
